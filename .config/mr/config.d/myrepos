[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/myrepos.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-myrepos.git myrepos
setpush  = vcsh myrepos remote set-url --add --push origin git@stillwell.me:mark/vcsh-homedir-myrepos.git
           vcsh myrepos remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-myrepos.git
           vcsh myrepos remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-myrepos.git
