[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/myrepos.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-myrepos.git myrepos
setpush  = vcsh myrepos remote set-url --push origin git@github.com:marklee77/vcsh-homedir-myrepos.git

