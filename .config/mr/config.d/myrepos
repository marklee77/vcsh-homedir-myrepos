[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/myrepos.git]
order    = 5
checkout = vcsh clone https://github.com/marklee77/vcsh-homedir-myrepos.git myrepos
commit   = vcsh myrepos commit -a
update   = vcsh myrepos pull
push     = vcsh myrepos push git@github.com:marklee77/vcsh-homedir-myrepos.git
status   = vcsh myrepos status
gc       = vcsh myrepos gc

