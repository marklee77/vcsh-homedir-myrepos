[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/powerline.git]
order    = 5
checkout = vcsh clone https://github.com/marklee77/vcsh-homedir-powerline.git powerline
commit   = vcsh powerline commit -a
update   = vcsh powerline pull &&
           pip install --user git+git://github.com/Lokaltog/powerline
push     = vcsh powerline push git@github.com:marklee77/vcsh-homedir-powerline.git
status   = vcsh powerline status
gc       = vcsh powerline gc

